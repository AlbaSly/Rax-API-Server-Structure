{
  "name": "rax-api-server-structure",
  "version": "0.0.1",
  "description": "API Server Structure for Medium/Large Express-Based Projects.",
  "main": ".dist/index.js",
  "scripts": {
    "dev": "concurrently -k \"tsc -w\" \"sleep 3 && nodemon ./dist/index.js\"",
    "w-node": "nodemon ./dist/index.js",
    "w-ts": "tsc -w",
    "build": "npx tsc",
    "start": "node dist/index.js"
  },
  "_moduleAliases": {
    "@core": "dist/core",
    "@environments": "dist/environments",
    "@modules": "dist/modules",
    "@server": "dist/server"
  },
  "keywords": [
    "backend",
    "api",
    "rest",
    "layout",
    "express"
  ],
  "author": "Raxel Arias",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "express-unless": "^2.1.3",
    "express-useragent": "^1.0.15",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "request-ip": "^3.3.0",
    "typeorm": "^0.3.17",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/express-useragent": "^1.0.5",
    "@types/nodemailer": "^6.4.13",
    "@types/request-ip": "^0.0.41",
    "@types/uuid": "^9.0.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
